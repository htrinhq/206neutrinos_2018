#!/usr/bin/env python3

from sys import argv, stderr
from utilities import helper, error

if __name__ == "__main__":
    data = []
    if len(argv) == 2 and argv[1] == "-h":
        helper()
    elif len(argv) != 5:
        error('invalid arguments.')
    else:
        try:
            for i in range(1, 5):
                data.append(int(argv[i]))
        except ValueError:
            error('Agruments must be integers.')
